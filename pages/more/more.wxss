/* pages/more/more.wxss */
.container{
  display: flex;
  flex-direction: column;
  text-align: center;
  align-items: center;
  width: 100%;
}

.surprise{
  display: flex;
  flex-direction: column;
  text-align: center;
  align-items: center;
  justify-content: space-between;
  width: 100%;
}

.title{
  color: #50bba7;
  font-size: 28rpx;
  height: 60rpx;
  line-height: 60rpx;
}

.list{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  height: 100rpx;
  line-height: 100rpx;
  width: 100%;
  border-bottom: 1rpx solid #f5f5f5;
}

.num{
  color: red;
}

.num-blue{
  color: blue;
}